<header>
    This is header
    <ul>
        <% if (locals.user){ %>
            <li><%= user.name %> </li>
            <li>
                <a href="/user/signout">Sign Out</a>
            </li>
        <% }else{ %>
            <!-- <%= console.log('this print statement coming from _header.ejs(Views)', locals.user) %> -->
            <li>
                <a href="/user/signin">Sign In</a>
            </li>

            <li>
                <a href="/user/signup">Sign Up</a>
            </li>
        
        <% } %>
    </ul>
</header>